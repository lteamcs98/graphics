	body

		script.
			console.log(#{challengeIdList});

		div(class='grid grid-pad')
			div(class='col-1-8')
				h2 Challenge ID
				ul
					each id in challengeIdList
						li(class='no-bg')= id
			div(class='col-1-2')
				h2 Challenge Name
				ul
					each name in challengeNameList
						li(class='no-bg')= name
			div(class='col-1-8')
				h2 Number Attempted
				ul
					each value in attemptedList
						li(class='no-bg')= value
			div(class='col-1-8')
				h2 Percentage Correct
				ul
					each value in percentageList
						li(class='no-bg')= value
